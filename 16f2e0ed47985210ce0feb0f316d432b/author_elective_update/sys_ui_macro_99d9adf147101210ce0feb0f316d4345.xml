        <xml><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">
    <a>
        <img src="x_1210081_school_0.whatsapp.jpg" width="40" height="30"
             onclick="userwindow('${ref}')"></img>
    </a>

    <script>
        function userwindow(reference) {
            alert('Image clicked! Preparing to open WhatsApp...');
            // Fetching values from the form fields directly
            var studentName = g_form.getValue('student_name');
            var mobileNumber = g_form.getValue('mobile_number');
            var marks = g_form.getValue('marks');
            var result = g_form.getValue('result');
            var grade = g_form.getValue('grade');
            var email = g_form.getValue('email');
			var link = 'https://schooldetils.netlify.app/'

			var demotext = 'Hi,\n' +
               'Student Name: ' + studentName + '\n' +
               'Mobile Number: ' + mobileNumber + '\n' +
               'Marks: ' + marks + '\n' +
               'Result: ' + result + '\n' +
               'Grade: ' + grade + '\n' +
'For More Details about Delhi Public School:'+ link + '\n' +
               'Email ID: ' + email ;
			      
            

            // Clean up the mobile number (if necessary)
            mobileNumber = mobileNumber.replace(/[^0-9]+/gi, '');

            // Debugging to verify values
            console.log("Mobile:", mobileNumber);
            console.log("Message:", demotext);

            // If mobile number exists, open WhatsApp link
            if (mobileNumber) {
				// alert("iffff")
                var whatsappURL = 'https://wa.me/' + mobileNumber + '?text=' + encodeURIComponent(demotext);
                //alert('Opening WhatsApp: ' + whatsappURL);
                open(whatsappURL, '_blank');
            } else {
                 alert('Mobile phone number not found.');
            }
        }
    </script>
</j:jelly>

]]></xml>
