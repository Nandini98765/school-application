        <messages/>
        <name>School Fee</name>
        <order/>
        <script><![CDATA[function onLoad() {
    // Get the selected grade when the form loads
    var grade = g_form.getValue('grade');
    
    // Populate the fee details based on the grade
    if (grade == 'Nursery' || grade == 'LKG' || grade == 'UKG') {
        // For Nursery, LKG, and UKG - excluding tuition fee
        g_form.setValue('student_fee', '50000');  // Assuming this is the total fee for these grades
        g_form.setValue('bus_fee', '3000');
        g_form.setValue('uniform_fee', '1200');
        g_form.setValue('books_fee', '800');
        
        // Clear the tuition fee since it's not applicable for these grades
        g_form.clearValue('tuetion_fee');

    } else if (grade >= '1' && grade <= '5') {
        // For 1st to 5th class
        g_form.setValue('student_fee', '100000');
        g_form.setValue('tuetion_fee', '18000');
        g_form.setValue('bus_fee', '5000');
        g_form.setValue('uniform_fee', '2000');
        g_form.setValue('books_fee', '1500');

    } else if (grade >= '6' && grade <= '10') {
        // For 6th to 10th class
        g_form.setValue('student_fee', '150000');
        g_form.setValue('tuetion_fee', '36000');
        g_form.setValue('bus_fee', '5000');
        g_form.setValue('uniform_fee', '4000');
        g_form.setValue('books_fee', '3500');
    } else {
        // If the grade is outside the expected range, clear the fields
        g_form.clearValue('student_fee');
        g_form.clearValue('tuetion_fee');
        g_form.clearValue('bus_fee');
        g_form.clearValue('uniform_fee');
        g_form.clearValue('books_fee');
    }
}
]]></script>
