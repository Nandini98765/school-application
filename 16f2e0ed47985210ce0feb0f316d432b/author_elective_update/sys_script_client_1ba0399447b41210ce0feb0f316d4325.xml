        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (isLoading || newValue === '') {
        return;
    }

    // Fetch values
    var tuitionfee1 = g_form.getValue("tuition_fee_term_1");
    var busfee1 = g_form.getValue("bus_fee_term_1");
    var totalduefee = g_form.getValue("u_integer_2");

alert(typeof(tuitionfee1))
    tuitionfee1 = parseFloat(tuitionfee1);
    busfee1 = parseFloat(busfee1);
    totalduefee = parseFloat(totalduefee);

    // Calculate term 1 fee and update the total due fee
    var term_1_fee = tuitionfee1 + busfee1;
    totalduefee = totalduefee - term_1_fee;

    // Set the updated total due fee in the form
    g_form.setValue("u_integer_2", totalduefee);

    // Optionally, save the form after setting the value
    g_form.save();
}
]]></script>
